Cell[TextData[{
 "With an explicit analytical Hamiltonian in hand, there\[CloseCurlyQuote]s \
nothing we can\[CloseCurlyQuote]t do. \n\nFor example, we can compute the \
correlator between fermions ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox["\[Psi]", 
        StyleBox["a", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "\\psi_a", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "eca99c25-18a9-49e3-890d-d9e39f2a622d"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox["\[Psi]", 
        StyleBox["b", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "\\psi_b", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "9aedf452-423a-496a-8e61-a459b3dee4ab"],
 ", separated by a time ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|
    "boxes" -> FormBox["\[Tau]", TraditionalForm], "errors" -> {}, "input" -> 
     "\\tau", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "11c53281-b293-457a-b7ed-8ae6861c0b02"],
 ".\n(7)   ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["G", "TI"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox["\[Psi]", 
              StyleBox["a", "TI"]], ",", 
             SubscriptBox["\[Psi]", 
              StyleBox["b", "TI"]], ",", "\[Tau]"}], ")"}], "\[LongEqual]", 
         FractionBox[
          RowBox[{
            StyleBox["T", "TI"], 
            StyleBox["r", "TI"], 
            RowBox[{"(", 
              RowBox[{
                SuperscriptBox[
                 StyleBox["e", "TI"], 
                 RowBox[{
                   RowBox[{"(", 
                    RowBox[{"-", "\[Beta]", "+", 
                    SuperscriptBox["\[Tau]", "\[Prime]"]}], ")"}], 
                   StyleBox["H", "TI"], "\[Lambda]"}]], 
                SubscriptBox["\[Psi]", 
                 StyleBox["a", "TI"]], 
                SuperscriptBox[
                 StyleBox["e", "TI"], 
                 RowBox[{"-", 
                   SuperscriptBox["\[Tau]", "\[Prime]"], 
                   StyleBox["H", "TI"]}]], 
                SubscriptBox["\[Psi]", 
                 StyleBox["b", "TI"]]}], ")"}]}], 
          RowBox[{
            StyleBox["T", "TI"], 
            StyleBox["r", "TI"], 
            RowBox[{"(", 
              RowBox[{
                SuperscriptBox[
                 StyleBox["e", "TI"], 
                 RowBox[{
                   RowBox[{"(", 
                    RowBox[{"-", "\[Beta]", "+", 
                    SuperscriptBox["\[Tau]", "\[Prime]"]}], ")"}], 
                   StyleBox["H", "TI"], "\[Lambda]"}]], 
                SuperscriptBox[
                 StyleBox["e", "TI"], 
                 RowBox[{"-", 
                   SuperscriptBox["\[Tau]", "\[Prime]"], 
                   StyleBox["H", "TI"]}]]}], ")"}]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "G_n (\\psi_a, \\psi_b, \\tau) = \\frac{Tr(e^{(-\\beta+\\tau')H \
\\lambda} \\psi_a e^{-\\tau' H \\lamda} \\psi_b)}{Tr(e^{(-\\beta+\\tau')H \
\\lambda} e^{-\\tau' H \\lamda})}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b60b533d-db74-400e-882b-e55ba9c4e270"],
 ", where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox["\[Tau]", "\[Prime]"], "\[LongEqual]", "\[Tau]"}], 
       TraditionalForm], "errors" -> {}, "input" -> "\\tau'=\\tau", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "28edecc8-5935-444e-8219-dac27c0ec182"],
 " if ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Tau]", ">", "0"}], TraditionalForm], "errors" -> {}, 
     "input" -> "\\tau>0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5af5ce99-6c7e-4d63-adc3-6b2460344f56"],
 "  and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox["\[Tau]", "\[Prime]"], "\[LongEqual]", "-", 
         "\[Tau]"}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\tau'=-\\tau", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f5e5594c-368a-42f9-94fd-1ee8e5076579"],
 " if ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Tau]", "<", "0"}], TraditionalForm], "errors" -> {}, 
     "input" -> "\\tau<0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b5c726b6-b8ea-49ff-916b-71a6211f00ca"],
 "."
