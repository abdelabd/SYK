Cell[TextData[{
 "Now we can find ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Psi]", 
          StyleBox["n", "TI"]], ",", 
         SubsuperscriptBox["\[Psi]", 
          StyleBox["n", "TI"], "\[Dagger]"]}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\psi_n, \\psi_n^\\dagger", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0f1aa6df-1e35-43f2-8c5a-2eba8fba408b"],
 ". Since each one is expensive to compute and will be called upon many \
times, it\[CloseCurlyQuote]s more efficient to precompute a look-up table for \
the first K states of both ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox["\[Psi]", 
        StyleBox["n", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "\\psi_n", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ebfd7fdb-c0d5-4f8a-b9c0-474bf8374045"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubsuperscriptBox["\[Psi]", 
        StyleBox["n", "TI"], "\[Dagger]"], TraditionalForm], "errors" -> {}, 
     "input" -> "\\psi_n^\\dagger", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "94aa9662-ac66-4b26-a57d-fb9e91080680"],
 "."
}], "Text",
 CellChangeTimes->{{3.8970061500530005`*^9, 
  3.8970062365194507`*^9}},ExpressionUUID->"d76156c1-82c7-49f7-a912-\
9a677f0b6e2a"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Psi]", "[", "n", "]"}], "=", 
     RowBox[{
      FractionBox["1", 
       SqrtBox["2"]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"c", "[", "n", "]"}], "+", 
        RowBox[{"cd", "[", "n", "]"}]}], ")"}]}]}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"n", ",", "Nc"}], "}"}]}], "]"}], "//", 
  "Timing"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Psi]", "[", 
      RowBox[{"Nc", "+", "n"}], "]"}], "=", 
     RowBox[{
      FractionBox["1", 
       SqrtBox["2"]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "I"}], " ", 
         RowBox[{"c", "[", "n", "]"}]}], "+", 
        RowBox[{"I", " ", 
         RowBox[{"cd", "[", "n", "]"}]}]}], ")"}]}]}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"n", ",", " ", "Nc"}], "}"}]}], "]"}], "//", 
  "Timing"}]}], "Input",
 CellChangeTimes->{{3.8968056732165823`*^9, 3.896805703647079*^9}, {
